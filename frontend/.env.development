# Backend URL for OAuth2 redirects (login/logout)
# API calls use Vite proxy (relative URLs) for cookie support
VITE_API_ENDPOINT=http://localhost:8080
VITE_KEYCLOAK_URL=https://keycloak.jobst24.de
VITE_KEYCLOAK_CLIENT_ID=resulter-vue-frontend
VITE_KEYCLOAK_REALM=resulter
VITE_IMPRESS_TEXT_DE=Impressums-Text
VITE_IMPRESS_TEXT_EN=Imprint-Text

# BFF Authentication Mode (true = BFF with session cookies, false = Keycloak with JWT tokens)
VITE_USE_BFF_AUTH=true
