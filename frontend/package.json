{
    "name": "resulter",
    "type": "module",
    "version": "4.6.3",
    "private": true,
    "packageManager": "pnpm@10.27.0",
    "scripts": {
        "dev": "node --localstorage-file=.node-localstorage node_modules/vite/bin/vite.js --mode development",
        "build": "nx exec -- echo 'Ran type-check and build-only'",
        "preview": "nx vite:preview",
        "build-only": "nx vite:build --mode production",
        "type-check": "vue-tsc --noEmit --project tsconfig.typecheck.json",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "clean": "rm -rf dist",
        "cleanjs": "find src -type f \\( -name '*.js' -o -name '*.d.ts' \\) ! -name 'vue-i18n.d.ts' ! -name 'shims-vue.d.ts' -delete",
        "bundle-visualizer": "pnpx vite-bundle-visualizer",
        "test:all": "pnpm test:unit && pnpm test:e2e",
        "test:unit": "vitest run",
        "test:unit:watch": "vitest watch",
        "test:unit:ui": "vitest --ui",
        "test:unit:coverage": "vitest run --coverage",
        "test:unit:file": "vitest run",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:chromium": "playwright test --project=chromium",
        "test:e2e:firefox": "playwright test --project=firefox",
        "test:e2e:webkit": "playwright test --project=webkit",
        "test:e2e:auth": "playwright test auth-setup.ts",
        "test:e2e:auth:headed": "playwright test auth-setup.ts --headed",
        "test:e2e:auth:debug": "playwright test auth-setup.ts --debug",
        "test:e2e:main": "playwright test main.spec.ts",
        "test:e2e:event": "playwright test event.spec.ts",
        "test:e2e:event-form": "playwright test event-form.spec.ts",
        "test:e2e:report": "playwright show-report",
        "test:e2e:trace": "playwright show-trace",
        "test:e2e:check": "bash scripts/run-e2e.sh",
        "test:cypress": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
        "test:cypress:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'"
    },
    "dependencies": {
        "@base2/pretty-print-object": "^1.0.2",
        "@fontsource/manrope": "^5.2.8",
        "@primeuix/themes": "^2.0.2",
        "@tanstack/vue-query": "^5.92.5",
        "@tanstack/vue-query-devtools": "^6.1.3",
        "@vueuse/core": "^14.1.0",
        "axios": "^1.13.2",
        "cors": "^2.8.5",
        "flag-icons": "^7.5.0",
        "moment": "^2.30.1",
        "moment-timezone": "^0.6.0",
        "pinia": "^3.0.4",
        "pinia-plugin-persistedstate": "^4.7.1",
        "primeflex": "^4.0.0",
        "primeicons": "^7.0.0",
        "primevue": "^4.5.4",
        "spring-filter-query-builder": "^1.1.1",
        "vue": "^3.5.26",
        "vue-i18n": "^11.2.8",
        "vue-pdf-embed": "^2.1.3",
        "vue-router": "^4.6.4"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^6.7.3",
        "@intlify/unplugin-vue-i18n": "^11.0.3",
        "@nx/eslint": "22.3.3",
        "@nx/playwright": "22.3.3",
        "@nx/vite": "22.3.3",
        "@nx/vue": "22.3.3",
        "@nx/web": "22.3.3",
        "@pinia/testing": "^1.0.3",
        "@playwright/test": "^1.57.0",
        "@rollup/plugin-dynamic-import-vars": "^2.1.5",
        "@rushstack/eslint-patch": "^1.15.0",
        "@tailwindcss/postcss": "^4.1.18",
        "@tsconfig/node18": "^18.2.6",
        "@types/jsdom": "^27.0.0",
        "@types/moment-duration-format": "^2.2.7",
        "@types/node": "^25.0.9",
        "@unocss/eslint-plugin": "^66.6.0",
        "@vitejs/plugin-vue": "^6.0.3",
        "@vitest/browser-playwright": "^4.0.16",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.8.1",
        "autoprefixer": "^10.4.23",
        "axe-core": "^4.11.1",
        "baseline-browser-mapping": "^2.9.15",
        "cypress": "^15.9.0",
        "dotenv": "^17.2.3",
        "eslint": "^9.39.2",
        "eslint-plugin-cypress": "^5.2.1",
        "eslint-plugin-format": "^1.2.0",
        "eslint-plugin-vue": "^10.6.2",
        "jsdom": "^27.4.0",
        "lint-staged": "^16.2.7",
        "npm-run-all2": "^8.0.4",
        "nx": "22.3.3",
        "playwright": "^1.57.0",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
        "sass": "^1.97.2",
        "simple-git-hooks": "^2.13.1",
        "start-server-and-test": "^2.1.3",
        "tailwindcss": "^4.1.18",
        "typescript": "~5.9.3",
        "unocss": "^66.5.12",
        "vite": "^7.3.1",
        "vite-plugin-vue-devtools": "^8.0.5",
        "vitest": "^4.0.16",
        "vitest-browser-vue": "^2.0.1",
        "vue-mess-detector": "^0.68.0",
        "vue-tsc": "^3.2.2"
    },
    "simple-git-hooks": {
        "pre-commit": "pnpm lint-staged"
    },
    "lint-staged": {
        "*": "eslint --fix"
    },
    "nx": {}
}
